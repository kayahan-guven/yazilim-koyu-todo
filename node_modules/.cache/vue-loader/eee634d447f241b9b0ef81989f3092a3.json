\n  import { mapActions } from 'vuex';\n\n  export default {\n      props: {\n          data: {\n            type: Object\n          },\n          index: {\n            type: Number,\n            required: true\n          }\n      },\n\n      data() {\n          return {\n            canEdit: false,\n            newItemName: ''\n          }\n      },\n\n      methods: {\n          ...mapActions([\n              'changeItemName',\n              'saveChangedName'\n          ]),\n\n          editName () {\n              this.canEdit = true;\n          },\n\n          saveChangedName () {\n                this.changeItemName({ index: this.index, listName: this.newItemName });\n                this.canEdit = false;\n          },\n      }\n  }\n</script>\n\n<style scoped>\n  .block {\n    display: flex;\n    position: relative;\n    margin-top: 15px;\n  }\n\n  svg {\n    margin-right: 10px;\n  }\n\n  .value {\n    position: absolute;\n    right: 18px;\n  }\n\n  .save-changed-name {\n    width: 40px;\n    height: 40px;\n  }\n\n  .item-list-container {\n    display: flex;\n    align-items: center;\n  }\n\n  .new-item {\n    margin-right: 10px;\n    width: 125px;\n  }\n</style>\n"]}]}